{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "keepUpdatedLabel": "renovate:keep-updated",
  "lockFileMaintenance": {
    "automerge": true,
    "enabled": true,
    "extends": [
      "schedule:daily"
    ]
  },
  "packageRules": [
    {
      "automerge": true,
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
  ],
  "rebaseLabel": "renovate:rebase",
  "stopUpdatingLabel": "renovate:disabled",
  "timezone": "Europe/Vienna"
}

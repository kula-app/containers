{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "regexManagers": [
    {
      "fileMatch": [".github/workflows/build-images.yml"],
      "matchStrings": ["(?<depName>.*)-rbenv:(?<currentValue>[\\d\\.]*)"],
      "datasourceTemplate": "docker"
    }
  ]
}
